    <elementParameter field="GUESS_SCHEMA" name="GUESS_SCHEMA" value="&quot;&quot;"/>
    <elementParameter field="MEMO_SQL" name="QUERY" value="&quot;&#xD;&#xA;select distinct * from&#xD;&#xA;(&#xD;&#xA;select &#xD;&#xA;/* Collaborateur HDSH*/&#xD;&#xA;CASE &#xD;&#xA;&#x9;WHEN (select count(*) from CONTENTIEUX c2&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;where c2.NO_SIT = 1&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;and c2.NO_DOSFACT = c.NO_DOSFACT&#xD;&#xA;                ) >= 1 THEN 'OUI'&#xD;&#xA;&#x9;ELSE 'NON'&#xD;&#xA;END as 'Collaborateur HDSH',&#xD;&#xA;df.NO_DOSFACT as 'No Dossier facturation' ,&#xD;&#xA;d.LB_POLITES as 'Civilité',&#xD;&#xA;d.NOM_CLIENT as 'Nom Client',&#xD;&#xA;d.PRE_CLIENT as 'Prénom Client',&#xD;&#xA;d.NOM_CLIENT2 as 'Nom Client 2',&#xD;&#xA;d.PRE_CLIENT2 as 'Prénom Client 2',&#xD;&#xA;pc.LB_ET_DTF as 'Etat',&#xD;&#xA;case loc.NBR_LGT_OCC_SLS &#xD;&#xA;&#x9;when 1 THEN 'OUI'&#xD;&#xA;&#x9;when 0 THEN 'NON'&#xD;&#xA;&#x9;else '-'&#xD;&#xA;end as SLS,&#xD;&#xA;/* Solde locataire à J-1 du lancement de la requête*/&#xD;&#xA;df.MT_SOLDLOY as Solde,&#xD;&#xA;df.ON_PRELEVE ,&#x9;&#x9;&#x9;&#x9;&#x9;&#xD;&#xA;c.LB_PROC as 'Libellé procedure',&#xD;&#xA;c.ID_DOSCTX as 'No Dossier CTX',&#xD;&#xA;c.LB_MOTOUV as 'Motif ouverture',&#xD;&#xA;c.LB_TYPSIT as 'Type Situation',&#xD;&#xA;c.DTD_DOSCTX as 'Date ouverture dossier CTX',&#xD;&#xA;c.DT_CREASIT as 'Date Création Situation',&#xD;&#xA;/*c.DTF_SIT as 'Date Fin Situation',*/&#xD;&#xA;c.NO_SIT as 'No Situation',&#xD;&#xA;c.LB_SITREP as 'Libellé Situation',&#xD;&#xA;pg.CD_PATRIM1 as 'Groupe',&#xD;&#xA;pg.CD_PATRIM as 'Ref Patrimoine',&#xD;&#xA;pg.LB_NATLOC as 'Nature Patrimoine',&#xD;&#xA;pg.LB_PATRIM3 as 'Adresse',&#xD;&#xA;pg.ADRPA_CP as 'CP',&#xD;&#xA;pg.ADRPA_LOC as 'Ville',&#xD;&#xA;pg.CD_SOCPROP,&#xD;&#xA;pg.LB_SOCPROP as 'Société Propriétaire',&#xD;&#xA;c.AFCTX_NOM_PER as 'Suivi par',&#xD;&#xA;/*Situation active*/&#xD;&#xA;act.NO_SIT as 'Situation active',&#xD;&#xA;act.LB_SITREP as 'Libellé situation active',&#xD;&#xA;act.DT_CREASIT as 'Date situation active',&#xD;&#xA;pc.DTF_CTRAT as 'Date fin contrat',&#xD;&#xA;pc.ADRDP_NORU as 'Adr parti: No rue',&#xD;&#xA;pc.ADRDP_CDRU as 'Adr parti: Code rue',&#xD;&#xA;pc.ADRDP_RUE1 as 'Adr parti: Rue1',&#xD;&#xA;pc.ADRDP_RUE2 as 'Adr parti: Rue2',&#xD;&#xA;pc.ADRDP_CP as 'Adr parti: CP',&#xD;&#xA;pc.ADRDP_LOC as 'Adr parti: Localité'&#xD;&#xA;from DOSS_FACT df&#xD;&#xA;left join PATRIMOINE_GIM pg on df.CD_PATRIM1 = pg.CD_PATRIM1 &#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and df.CD_PATRIM2 = pg.CD_PATRIM2&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and df.CD_PATRIM3 = pg.CD_PATRIM3&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and df.CD_PATRIM4 = pg.CD_PATRIM4&#xD;&#xA;left join CONTENTIEUX c on c.NO_DOSFACT = df.NO_DOSFACT &#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and df.CD_PATRIM1 = c.CD_PATRIM1 &#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and df.CD_PATRIM2 = c.CD_PATRIM2&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and df.CD_PATRIM3 = c.CD_PATRIM3&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and df.CD_PATRIM4 = c.CD_PATRIM4&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and c.CD_ETAT_CTX = 'O'&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and c.LB_PROC &lt;> 'A NE PLUS UTILISER'&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;/*and c.DT_CREASIT &lt;= '31/12/2020'*/&#xD;&#xA;/*Situaction active*/&#xD;&#xA;left join CONTENTIEUX act on act.NO_DOSFACT = df.NO_DOSFACT &#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and df.CD_PATRIM1 = act.CD_PATRIM1 &#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and df.CD_PATRIM2 = act.CD_PATRIM2&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and df.CD_PATRIM3 = act.CD_PATRIM3&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and df.CD_PATRIM4 = act.CD_PATRIM4&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and act.CD_ETAT_CTX = 'O'&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and act.ON_DERSITAC = 'O'&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and act.LB_PROC &lt;> 'A NE PLUS UTILISER'&#x9;&#xD;&#xA;left join DOSS_CLIEN d on df.ID_DOSCLIE = d.ID_DOSCLIE&#xD;&#xA;/*--- Pour la recherche de SLS, on devrait pouvoir trouver autrement ...*/&#xD;&#xA;left join SID_LOC_PRE_GIM loc on loc.ID_DOSCLIE = df.ID_DOSCLIE &#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and df.CD_PATRIM1 = loc.CD_PATRIM1 &#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and df.CD_PATRIM2 = loc.CD_PATRIM2&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and df.CD_PATRIM3 = loc.CD_PATRIM3&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and df.CD_PATRIM4 = loc.CD_PATRIM4&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and loc.DT_ANNEE = year(dateadd(month, -1,getdate()))&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and loc.DT_MOIS = month(dateadd(month, -1,getdate()))&#xD;&#xA;left join PAT_CONTRA pc on  pc.CD_PATRIM1 =df.CD_PATRIM1 &#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and pc.CD_PATRIM2 =df.CD_PATRIM2 &#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and pc.CD_PATRIM3 =df.CD_PATRIM3 &#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and pc.CD_PATRIM4 =df.CD_PATRIM4  &#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and df.NO_DOSFACT = pc.NO_DOSFACT&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and pc.LB_ET_DTD = 'Présent'&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and pc.DTD_CTRAT &lt;= EOMONTH(getdate(),-1)&#xD;&#xA;where &#xD;&#xA;/*-- Société propriétaire&#xD;&#xA;--pg.CD_SOCPROP not in (1, 6)*/&#xD;&#xA;d.ON_PSEUCLI = 'N'&#xD;&#xA;/*-- Dernière situation en date / Si même date, situation avec le nombre le plus élevé  */&#xD;&#xA;and not exists (select 1 from CONTENTIEUX c2&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;where c2.NO_DOSFACT = c.NO_DOSFACT&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;and c2.CD_ETAT_CTX = 'O'&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;and c2.LB_PROC &lt;> 'A NE PLUS UTILISER'&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;/*--and c2.DT_CREASIT &lt;= '31/12/2020'*/&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;and (c2.DT_CREASIT > c.DT_CREASIT &#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;OR (c2.NO_SIT > c.NO_SIT&#xD;&#xA;                &#x9;and c2.DT_CREASIT = c.DT_CREASIT)&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;)&#xD;&#xA;                )&#xD;&#xA;/*-- Date de contrat le plus récent               */&#xD;&#xA;and not exists (select 1 from PAT_CONTRA pc2&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;where pc2.DTD_CTRAT > pc.DTD_CTRAT&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;and pc2.NO_DOSFACT = pc.NO_DOSFACT&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;and pc2.DTD_CTRAT &lt;= EOMONTH(getdate(),-1)&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;and pc2.CD_PATRIM = pc.CD_PATRIM&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;and pc2.LB_ET_DTD = 'Présent'&#xD;&#xA;                )&#xD;&#xA;) BASE&#xD;&#xA;&quot;"/>
    <elementParameter field="LABEL" name="NOTE" value="This option only applies when deploying and running in the Talend Runtime" show="false"/>
